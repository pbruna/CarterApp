<!DOCTYPE html>
<html>
<head>
  <title>Carter</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="navbar">
	<div class="navbar-inner">
		<div class="container">
			<a class="brand" href="#">
				Carter
			</a>
		</div>
	</div>
</div>
<div class="container-fluid">
	<% flash.each do |name, msg| %>
	  <div class="alert alert-<%= name == :notice ? "success" : "error" %>" style="text-align: center;">
	    <a class="close" data-dismiss="alert">Ã—</a>
	    <%= msg %>
	  </div>
	<% end %>
    <div id="main">
      <%= yield %>
    </div>
  </div>
</div>


</body>
</html>
